<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" fx:controller="javafx.MainMenuController" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	prefHeight="400.0" prefWidth="600.0" spacing="30.0" >
   <children>
      <Text id="titleText" text="Play Chess!"/>
      <Button fx:id="playGameButton" onAction="#handleClick" text="Play Game"/>
      <Button fx:id="quitGameButton" onAction="#handleQuit" text="Exit Application"/>
   </children>
</VBox>
